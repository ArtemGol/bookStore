(addToCart(obj))\r\n    }\r\n    return (\r\n\r\n        <Card>\r\n            <Image src={image} wrapped ui={false}/>\r\n            <Card.Content>\r\n                <Card.Header>{title}</Card.Header>\r\n                <Card.Meta>\r\n                    <span className='date'>{author}</span>\r\n                </Card.Meta>\r\n            </Card.Content>\r\n            <Card.Content extra>\r\n                <a>\r\n                    <Icon name='rub'/>\r\n                    {addedCount > 0 ? price * addedCount : price}({addedCount})\r\n                </a>\r\n            </Card.Content>\r\n                <Card.Content extra>\r\n                    <div className='ui two buttons'>\r\n                        <Button basic color='red' onClick={deleteBook}>\r\n                            Delete\r\n                        </Button>\r\n                        <Button basic color='green' onClick={addBook}>\r\n                            Add\r\n                        </Button>\r\n                    </div>\r\n                </Card.Content>\r\n        </Card>\r\n\r\n    )\r\n}\r\n\r\nexport default CardMainComponent","C:\\react11\\projects\\second-project\\src\\components\\Card\\CartContainer.jsx",[],"C:\\react11\\projects\\second-project\\src\\redux\\reducers\\alert-reducer.js",[],"C:\\react11\\projects\\second-project\\src\\redux\\actions\\alert.js",[],"C:\\react11\\projects\\second-project\\src\\components\\Alert\\Alert.jsx",["112","113","114"],"C:\\react11\\projects\\second-project\\src\\components\\Alert\\AlertContainer.jsx",[],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":17,"column":12,"nodeType":"121","endLine":17,"endColumn":14,"suggestions":"122"},{"ruleId":"123","severity":1,"message":"124","line":27,"column":17,"nodeType":"125","endLine":27,"endColumn":20},{"ruleId":"126","severity":1,"message":"127","line":3,"column":16,"nodeType":"128","messageId":"129","endLine":3,"endColumn":